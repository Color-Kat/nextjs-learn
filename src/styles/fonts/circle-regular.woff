            placemark;
            
            function init() {
            	map = new ymaps.Map("map", {
            		center: [55.724191, 37.504853],
            		zoom: 16,
            		controls: []
            	});
            	placemark = new ymaps.Placemark([55.724191, 37.504853], {
            			balloonContentHeader: 'Бизнесс-центр Victory Park',
            			balloonContentBody: 'Адрес: г. Москва, Минская улица, 2Ж<br>' + 'Телефон: +7 (495) ' +
            			'21-21-799<br>' + 'Email: info@minskayaplaza.ru'
            		},
            		{
            			iconLayout: 'default#image',
            			iconImageHref: 'img/map-marker.png',
            			iconImageSize: [50, 63],
            			iconImageOffset: [-30, -60]
            		}); 
            	map.geoObjects.add(placemark);
            	map.behaviors.disable('scrollZoom');
            	map.behaviors.disable('drag');
            }
